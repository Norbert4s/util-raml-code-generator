#%RAML 1.0 DataType
description: User
discriminator: type
properties:
  id:
    type: string
    required: false
  type:
    type: string
    enum:
      - legal
      - natural
    required: true
    description: User's type
  created_timestamp:
    (datetime_timestamp):
    type: integer
    required: true
  created_datetime:
    type: datetime
  created_date_only:
    type: date-only
  created_time_only:
    type: time-only
  created_datetime_only:
    type: datetime-only

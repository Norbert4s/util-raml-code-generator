<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity repository-class="Vendor\Test\TransferSurveillanceApiBundle\Repository\ReviewRepository"
            name="Vendor\Test\TransferSurveillanceApiBundle\Entity\Review" table="transfer_surveillance_api_reviews">

        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="reviewerUserId" column="reviewer_user_id" nullable="true" type="string"/>
        <field name="comment" column="comment" nullable="true" type="string"/>
        <field name="internalComment" column="internal_comment" nullable="true" type="string"/>
        
    </entity>
</doctrine-mapping>

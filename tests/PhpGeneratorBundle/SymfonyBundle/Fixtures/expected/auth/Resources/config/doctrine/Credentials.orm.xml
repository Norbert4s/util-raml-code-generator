<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity repository-class="Vendor\Test\AuthApiBundle\Repository\CredentialsRepository"
            name="Vendor\Test\AuthApiBundle\Entity\Credentials" table="auth_api_credentials">

        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>
        <field name="username" column="username" nullable="false" type="string"/>
        <field name="password" column="password" nullable="false" type="string"/>
        
    </entity>
</doctrine-mapping>

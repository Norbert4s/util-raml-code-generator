{
    "output": "raml-code-generator.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "algorithm": "SHA1",
    "finder": [
        {
            "name": ["*.php", "*.xml", "*.yml", "*.xsd", "*.db", "*.twig"],
            "exclude": ["test", "Tests", "tests"],
            "in": ["src", "vendor", "app"]
        }
    ],
    "blacklist": [
        "app/config/publish_parameters.yml"
    ],
    "files": [
        "CHANGELOG.md"
    ],
    "git-tag": "package_version",
    "stub": "raml-code-generator"
}

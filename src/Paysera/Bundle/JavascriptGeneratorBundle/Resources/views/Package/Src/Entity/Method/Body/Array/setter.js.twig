{% if property.itemsType in property.simpleTypes -%}
    this.set('{{ property.name }}', {{ property.name|to_variable_name }});
{% else -%}
    let data = [];
        for (let entity of {{ property.name|to_variable_name }}) {
            data.push(entity.getData());
        }
        this.set('{{ property.name }}', data);
{% endif %}

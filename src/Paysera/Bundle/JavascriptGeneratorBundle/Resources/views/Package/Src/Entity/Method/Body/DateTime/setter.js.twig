
    /**
     * @param {% include get_method_argument_type_template(property) with { 'property': property } only %}
     */
    {{ js_generate_setter_name(property) }}({{ property.name|to_variable_name }}) {
        {% if js_resolve_date_type_format(property) == null -%}
        this.set('{{ property.name }}', Math.floor({{ property.name|to_variable_name }}.toMillis()/1000));
{% else -%}
        this.set('{{ property.name }}', {{ property.name|to_variable_name }}.toFormat("{{ js_resolve_date_type_format(property)|raw }}"));
{% endif %}
    }


    /**
     * @param {% include get_method_argument_type_template(property) with { 'property': property } only %}
     */
    {{ js_generate_setter_name(property) }}({{ property.name|to_variable_name }}) {
    {% if property.required == false %}
    if ({{ property.name|to_variable_name }} === null) {
            this.set('{{ property.name }}', null);
            return;
        }
    {% endif %}
    this.set('{{ property.name }}', btoa({{ property.name|to_variable_name }}));
    }

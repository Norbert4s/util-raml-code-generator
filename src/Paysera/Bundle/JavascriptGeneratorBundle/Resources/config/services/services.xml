<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="paysera_javascript_generator.language_code_generator"
                 class="Paysera\Bundle\CodeGeneratorBundle\Service\Generator\LanguageCodeGenerator">
            <tag name="paysera_code_generator" language="js"/>
        </service>

        <service id="paysera_javascript_generator.service.package_name_resolver"
                 class="Paysera\Bundle\JavascriptGeneratorBundle\Service\PackageNameResolver" />

        <service id="paysera_javascript_generator.service.client_name_resolver"
                 class="Paysera\Bundle\JavascriptGeneratorBundle\Service\ClientNameResolver">
            <argument type="service" id="paysera_php_generator.string_converter" />
        </service>
    </services>
</container>

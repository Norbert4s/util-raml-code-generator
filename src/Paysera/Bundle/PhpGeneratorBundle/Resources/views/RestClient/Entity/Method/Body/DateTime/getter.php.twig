
{% if property.required == false %}
        if ($this->get('{{ property.name }}') === null) {
            return null;
        }
{% endif %}
{% if property.type == constant('TYPE_INTEGER', property) %}
        return (new \DateTimeImmutable())->setTimestamp($this->get('{{ property.name }}'));
{% else %}
        return (new \DateTimeImmutable($this->get('{{ property.name }}')));
{% endif %}

<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="paysera_php_generator.language_code_generator"
                 class="Paysera\Bundle\CodeGeneratorBundle\Service\Generator\LanguageCodeGenerator">
            <tag name="paysera_code_generator" language="php"/>
        </service>

        <service id="paysera_php_generator.namespace_helper"
                 class="Paysera\Bundle\PhpGeneratorBundle\Service\NamespaceHelper">
        </service>
    </services>
</container>
